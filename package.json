{
  "name": "peacemakr-website",
  "version": "1.0.0",
  "description": "peacemakr.io website",
  "main": "index.js",
  "scripts": {
    "clean-localdev": "rm -rf ./.cache ./localdev",
    "clean-prod": "rm -rf ./.cache ./docs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean-prod && parcel build -d ./docs src/index.html --global navbar.js --public-url / --log-level 5 && cp CNAME favicon.ico ./docs",
    "localdev": "npm run clean-localdev && parcel -d ./localdev src/index.html --public-url /",
    "start": "npm run build && parcel ./docs/index.html"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://peacemakr.io",
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^9.8.6",
    "postcss": "^7.0.35",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "tailwindcss-aspect-ratio": "latest"
  },
  "devDependencies": {
    "parcel-bundler": "latest",
    "postcss-modules": "latest"
  }
}
